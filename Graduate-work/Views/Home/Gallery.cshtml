@model List<AddPictureViewModel>
<html lang="">
<head>
    <title>@Graduate_work.Localize.language.Layout_title | @Graduate_work.Localize.language.Layout_dropdown | @Graduate_work.Localize.language.Layout_gallery</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link href="/css/_Layout.css" rel="stylesheet" type="text/css" media="all">
</head>
<body id="top">

    <div class="bgded" style="background-image:url(/wwwroot/images/demo/backgrounds/01.png);">

        <div class="wrapper row1">
            <header id="header" class="hoc clear">

                <nav id="mainav" class="fl_right">
                    <ul class="clear">
                        <li class="active">
                            <a href="/User/Index">@Graduate_work.Localize.language.Layout_title</a>
                        </li>
                        <li>
                            <a class="drop" href="#">@Graduate_work.Localize.language.Layout_dropdown</a>
                            <ul>
                                <li><a href="/User/Profile">@Graduate_work.Localize.language.Layout_profile</a></li>
                                <li class="active"><a href="#">@Graduate_work.Localize.language.Layout_gallery</a></li>
                                <li><a href="">@Graduate_work.Localize.language.Link_Text</a></li>
                            </ul>
                        </li>
                        <li>
                            <a class="drop" href="#">Dropdown</a>
                            <ul>
                                <li><a href="#">Level 2</a></li>
                                <li>
                                    <a class="drop" href="#">Level 2 + Drop</a>
                                    <ul>
                                        <li><a href="#">Level 3</a></li>
                                        <li><a href="#">Level 3</a></li>
                                        <li><a href="#">Level 3</a></li>
                                    </ul>
                                </li>
                                <li><a href="#">Level 2</a></li>
                            </ul>
                        </li>
                        <li><a href="#">@Graduate_work.Localize.language.Link_Text</a></li>
                        <li><a href="/User/Logout">@Graduate_work.Localize.language.UserController_Logout</a></li>
                    </ul>
                </nav>
            </header>
        </div>

        <div id="breadcrumb" class="hoc clear">

            <ul>
                <li><a href="/User/Profile">@Graduate_work.Localize.language.Layout_profile</a></li>
                <li><a href="#">Gallery</a></li>
                <li><a href="#">@Graduate_work.Localize.language.Link_Text</a></li>
            </ul>

        </div>

    </div>

    <div class="wrapper row3">
        <main class="hoc container clear">

            <div class="content">

                <div id="gallery">
                    <figure>
                        <header class="heading">Gallery Title Goes Here</header>
                        <ul class="nospace clear">
                            @foreach (var picture in Model)
                            {
                                <li class="one_quarter"><a href="#"><img src=@picture.Url alt=""></a></li>
                            }

                        </ul>
                        <figcaption>@Graduate_work.Localize.language.Gallery_UserPictures</figcaption>
                    </figure>
                </div>

                <form action="/Picture/Add" method="post">
                    <nav class="pagination">
                        <ul>
                            <li><a href="#">&laquo; Previous</a></li>
                            <li><a href="/Home/Gallery" value=1>1</a></li>
                            <li><a href="/Home/Gallery" value=2>2</a></li>
                            <li><strong>&hellip;</strong></li>
                            <li><a href="/Home/Gallery" value=6>6</a></li>
                            <li><a href="/Home/Gallery" value=7>7</a></li>
                            <li><a href="/Home/Gallery" value=8>8</a></li>
                            <li><a href="/Home/Gallery" value=9>9</a></li>
                            <li><strong>&hellip;</strong></li>
                            <li><a href="/Home/Gallery" value=14>14</a></li>
                            <li><a href="/Home/Gallery" value=15>15</a></li>
                            <li><a href="#">Next &raquo;</a></li>
                        </ul>
                    </nav>
                </form>
            </div>

            <div class="clear"></div>
        </main>
    </div>
</body>
</html>